{
  "name": "wiggum-cli",
  "version": "0.2.1",
  "description": "AI-powered feature development loop CLI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ralph": "./bin/ralph.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "cp -r src/templates dist/",
    "dev": "tsc --watch",
    "start": "node bin/ralph.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "ai",
    "development",
    "automation",
    "feature-loop",
    "code-generation",
    "tech-stack-detection"
  ],
  "author": "Ralph CLI Contributors",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.15",
    "@ai-sdk/openai": "^3.0.12",
    "@clack/prompts": "^0.7.0",
    "ai": "^6.0.41",
    "cfonts": "^3.2.0",
    "commander": "^12.1.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
